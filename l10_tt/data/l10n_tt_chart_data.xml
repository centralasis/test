<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <!-- Chart template -->
        <record id="l10n_tt" model="account.chart.template">
            <field name="name">Trinidad &amp; Tobago Tax and Account Chart Template (by Centralasis)</field>
            <field name="bank_account_code_prefix">1200</field>
            <field name="cash_account_code_prefix">1210</field>
            <field name="transfer_account_code_prefix">1220</field>
            <field name="code_digits">6</field>
            <field name="currency_id" ref="base.TTD"/>
            <field name="country_id" ref="base.tt"/>
        </record>
</odoo>
